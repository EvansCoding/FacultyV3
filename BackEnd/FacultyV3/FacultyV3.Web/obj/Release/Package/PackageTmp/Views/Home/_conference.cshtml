@model List<FacultyV3.Core.Models.Entities.Conference>
<style>
    .owl-carousel .item-owl-carousel img {
        height: 130px !important;
    }
</style>
<section id="mu-from-blog" style="padding-top: 0px">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="mu-from-blog-area">
                    <!-- start title -->
                    <div class="mu-title">
                        <h2>Hợp Tác</h2>
                    </div>
                    <div class="mu-from-blog-content">
                        <div class="owl-carousel" style="background-color: #ccc; margin-top: 10px; margin-bottom: 10px">
                            @foreach (var item in Model)
                            {
                                <div class="item-owl-carousel"> <a href="@item.Url_Link"><img src="@item.Url_Image" /></a>  </div>
                            }
                        </div>
                    </div>
                    <!-- end from blog content   -->
                </div>
            </div>
        </div>
    </div>
</section>
